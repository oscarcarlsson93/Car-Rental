@model List<Biluthyrning.Models.Customer>

<br />
<div>
    <a asp-action="Create">Lägg till ny kund</a>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                Förnamn
            </th>
            <th>
                Efternamn
            </th>
            <th>
                Personnummer
            </th>
            <th>
                Förmånsnivå
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PersonalNumber)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemberLevel)

            </td>
            <td>

                <a asp-action="AllBookings" asp-route-id="@item.Id">Se alla bokningar</a>

            </td>

        </tr>
        }
    </tbody>
</table>
